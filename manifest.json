{
    "manifest_version": 3,
    "name": "Moodle Filter",
    "version": "1.1",
    "description": "Filter out class from Moodle",
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "options_page": "options.html",
    "content_scripts": [
        {
            "js": ["scripts/filter.js"],
            "matches": [
                "https://moodle.hku.hk/*"
            ]
        }
    ]
}